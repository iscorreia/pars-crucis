<li class="inline-action inline-ammo">
  {{log this}}
  <span class="inline-label">{{localize "PC.ammo.label"}}:</span>
  <button
    type="button"
    class="pick-ammo inline-button"
    data-action="ammoPicker"
    data-item-id="{{itemId}}"
  >
    {{#if this.selectedAmmo}}
      <img
        class="inline-icon"
        src="{{this.selectedAmmo.img}}"
        width="16"
        height="16"
      />
      <span
        class="inline-action-name inline-selected"
      >{{this.selectedAmmo.name}}</span>
    {{else}}
      <span class="inline-action-name">{{localize "PC.selectAmmo"}}</span>
    {{/if}}
  </button>
  {{#if (not (isEmpty this.selectedAmmo.system.keywords))}}
    <span class="item-keywords">
      {{#each this.selectedAmmo.system.keywords}}
        {{> systems/pars-crucis/templates/partials/pc-keyword.hbs}}
      {{/each}}
    </span>
  {{/if}}
  {{#if (gt this.system.ammo.capacity 0)}}
    <span class="inline-label">{{localize "PC.capacity"}}</span>
    <span class="inline-value">
      {{this.system.ammo.loaded}}/{{this.system.ammo.capacity}}
    </span>
  {{/if}}
  {{#if this.selectedAmmo.effect}}
    Efeito aqui
  {{/if}}
</li>